{
    "name": "mohammadalhallaq/change-and-notify",
    "description": "This is my package change-and-notify",
    "keywords": [
        "MohammadAlhallaq",
        "laravel",
        "change-and-notify"
    ],
    "homepage": "https://github.com/mohammadalhallaq/change-and-notify",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad alhallaq",
            "email": "mohammad.alhallaq1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "MohammadAlhallaq\\ChangeAndNotify\\": "src",
            "MohammadAlhallaq\\ChangeAndNotify\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MohammadAlhallaq\\ChangeAndNotify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohammadAlhallaq\\ChangeAndNotify\\ChangeAndNotifyServiceProvider"
            ],
            "aliases": {
                "ChangeAndNotify": "MohammadAlhallaq\\ChangeAndNotify\\Facades\\ChangeAndNotify"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}